import java.io.*;
import java.util.*;
class fibonacci
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
System.out.println("enter the number:");
int n=s.nextInt();
int n1=0,n2=1,n3;
for(int i=1;i<=n;i++)
{
n3=n1+n2;
n2=n3;
n1=n2;
}
System.out.println("fibonacci series is"+n3);
}
}
