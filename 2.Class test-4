import java.util.*;
class Prime
{
public static void main(String args[])
{
int pri=0,com=0;
int arr[]={4,54,29,71,7,59,98,23};
for(int i=0;i<arr.length;i++)
{
int c=0;
for(int j=0;j<=i;j++)
{
if(arr[i]%2==0)
c++;
}
if(c>1)
{
com++;
}
else
{
pri++;
}
}
System.out.println("composite:"+com);
System.out.println("prime:"+pri);
}
}
