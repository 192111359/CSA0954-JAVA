import java.util.*;
class Factor
{
public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int num=s.nextInt();
		int n=s.nextInt();
		int a[]=new int[100];
		int x=0;
		for(int i=1;i<=num;i++)
		{
		 if(num%i==0) {
		 a[x] = i;
		 x++;
		 }
		}
		System.out.println("Number of factors = "+x);
		System.out.println(n+" factor of "+num+" = "+a[n-1]);
	}

}
